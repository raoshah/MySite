{
  "builds": [
    {
      "src": "mysite/vercel_wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "headers": { "cache-control": "public,max-age=31536000,immutable" }, "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "mysite/vercel_wsgi.py" }
  ],
  "buildCommand": "python manage.py collectstatic --noinput"
}
